{
    "workbench.colorTheme": "Kanagawa",
    "workbench.iconTheme": "material-icon-theme",
    "files.associations": {
        "*.jsx": "javascriptreact",
        ".env*": "dotenv",
        "exclude": "raw",
        ".env": "env",
        "*.txt": "tex"
    },
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "remote.SSH.remotePlatform": {
        "103.80.240.162": "linux"
    },
    "terminal.integrated.fontFamily": "Macon",
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {},
    // "autoDayNightThemeSwitcher.dayTheme": "Tokyo Night Light Frameless",
    // "autoDayNightThemeSwitcher.nightTheme": "Tokyo Night Frameless",
    "notebook.formatOnSave.enabled": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "editor.fontFamily": "'Dank Mono'",
    "editor.mouseWheelZoom": true,
    // "editor.fontLigatures": "'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
    // "editor.fontLigatures": "'calt', 'ss01'",
    // "editor.fontLigatures": false,
    "security.workspace.trust.enabled": false,
    "[python]": {
        "editor.defaultFormatter": null
    },
    "editor.unicodeHighlight.allowedCharacters": {
        "ู": true,
        "ุง": true
    },
    "python.languageServer": "Pylance",
    "sonarlint.rules": {
        "Web:S4084": {
            "level": "off"
        },
        "javascript:S125": {
            "level": "off"
        },
        "javascript:S4123": {
            "level": "off"
        },
        "python:S1192": {
            "level": "off"
        },
        "python:S125": {
            "level": "off"
        },
        "python:S1066": {
            "level": "off"
        },
        "python:S1542": {
            "level": "off"
        },
        "javascript:S3776": {
            "level": "off"
        },
        "python:S2208": {
            "level": "off"
        },
        "javascript:S1121": {
            "level": "off"
        },
        "javascript:S3358": {
            "level": "off"
        },
        "javascript:S6582": {
            "level": "off"
        },
        "typescript:S3358": {
            "level": "off"
        }
    },
    "editor.linkedEditing": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "keyword",
                    "constant",
                    "entity",
                    "string",
                    "support",
                    "storage",
                    "meta",
                    "variable",
                    "storage.modifier",
                    "storage.type.class.js",
                    "storage.type.js"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            }
        ]
    },
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.ignoreErrorLanguages": ["*"],
    "indentRainbow.colors": [
        "rgba(255,255,64,0.5)",
        "rgba(127,255,127,0.5)",
        "rgba(255,127,255,0.5)",
        "rgba(79,236,236,0.5)"
    ],
    "workbench.startupEditor": "none",
    "editor.tabCompletion": "on",
    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    "emmet.triggerExpansionOnTab": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "javascript.suggest.completeFunctionCalls": true,
    "typescript.suggest.completeFunctionCalls": true,
    "tabnine.experimentalAutoImports": true,
    "window.titleBarStyle": "custom",
    "vscode_vibrancy.theme": "Solarized Dark+",
    "[scss]": {
        "editor.defaultFormatter": null
    },
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "terminal.integrated.env.osx": {},
    "todo.colors.light": {
        "tag": "#afa65a",
        "done": "#038c01",
        "project": "#003eff",
        "cancelled": "#ff1902"
    },
    "todo.colors.dark": {
        "done": "#0aff00",
        "project": "#00c3ff",
        "cancelled": "#ba1100"
    },
    "workbench.editor.enablePreview": false,
    "files.exclude": {
        "**/.git": false
    },
    "git.openRepositoryInParentFolders": "always",
    "redhat.telemetry.enabled": true,
    "colorize.include": [
        "**/*.css",
        "**/*.scss",
        "**/*.sass",
        "**/*.less",
        "**/*.styl",
        "**/*.js"
    ],
    // "console-ninja.showWhatsNew": false,
    // "console-ninja.featureSet": "Community",
    "[env]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[plaintext]": {
        "editor.unicodeHighlight.ambiguousCharacters": false,
        "editor.unicodeHighlight.invisibleCharacters": false
    },
    "[dotenv]": {},
    "editor.fontVariations": false,
    "editor.fontWeight": "500",
    "editor.fontSize": 14,
    // "autoDayNightThemeSwitcher.autoToggleTimeNightBegin": "17:30",
    "explorer.compactFolders": false,
