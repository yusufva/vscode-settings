    "[tex]": {},
    "txtsyntax.associations": [
        ".ini",
        ".cf",
        ".cnf",
        ".conf",
        ".cfg",
        ".unx",
        ".eds",
        ".log",
        ".out",
        ".err",
        ".inf",
        ".info",
        ".tmp",
        ".temp",
        ".file",
        ".repo",
        ".plain",
        ".desktop",
        ".properties",
        ".txt"
    ],
    "[wat]": {},
    "txtsyntax.defaultHighlightMode": 0,
    "todo.embedded.exclude": [
        "**/.*",
        "**/.*/**",
        "**/_output/**",
