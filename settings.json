    "prettier.tabWidth": 4,
    "window.zoomLevel": 1,
    "tailwind-fold.autoFold": false,
    "tailwind-fold.foldStyle": "QUOTES",
    "class-collapse.classCollapseToggle": false,
    "class-collapse.openCollapseOnLineSelected": false,
    "prisma.showPrismaDataPlatformNotification": false
